<?xml version="1.0" encoding="utf-8" ?>
<!-- XSD manual extracted from package NLog.Schema: https://www.nuget.org/packages/NLog.Schema-->
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xsi:schemaLocation="NLog NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true">

  <!-- the targets to write to -->
  <targets>
    <!-- write logs to file -->
    <target xsi:type="File" name="fileLog" fileName="fsw.log"
            layout="${date}|${pad:padding=5:inner=${level:uppercase=true}}|${logger}|${message} ${exception}|${all-event-properties}" />
    <target xsi:type="Console" name="consoleLog"
            layout="${date}|${pad:padding=5:inner=${level:uppercase=true}}|${logger}|${message} ${exception}" />
  </targets>

  <!-- rules to map from logger name to target -->
  <rules>
    <logger name="*" minlevel="Debug" writeTo="fileLog" />-->
    <logger name="*" minlevel="Debug" writeTo="consoleLog" />
  </rules>
</nlog>